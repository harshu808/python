# module1.py

from my_package import module1, module2


def function1():
    return "Function 1 from module1"


# module2.py

def function2():
    return "Function 2 from module2"


# main.py
# Importing modules from the package

# Using functions from module1
result1 = module1.function1()
print(result1)

# Using functions from module2
result2 = module2.function2()
print(result2)
